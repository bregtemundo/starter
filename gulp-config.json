{
  "server": "drupal.docker.localhost:8000",
  "sourcemaps": true,
  "css": {
    "src": "src/css",
    "dest": "dist/css",
    "paths": [
      "node_modules/susy/sass",
      "node_modules/breakpoint-sass/stylesheets",
      "node_modules/sass-easing",
      "node_modules/family.scss/source/src",
      "node_modules/typi/scss",
      "node_modules/normalize-scss/sass",
      "node_modules/megatype",
      "node_modules/smooth-scrollbar/dist",
      "node_modules/flickity/dist",
      "node_modules/jquery.mmenu/dist",
      "node_modules/jquery.mmenu/src",
      "node_modules/superfish/src/css"
    ],
    "support": [
      "last 3 versions"
    ]
  },
  "js": {
    "src": "src/js",
    "dest": "dist/js",
    "paths": [
      "./node_modules",
      "./node_modules/barba.js/dist",
      "./node_modules/jquery/dist",
      "./src/js",
      "./node_modules/webfontloader",
      "./node_modules/scrollmagic/scrollmagic/minified",
      "./node_modules/scrollmagic/scrollmagic/uncompressed/plugins",
      "./node_modules/jquery.mmenu/dist",
      "./node_modules/match-media",
      "./node_modules/superfish/src/js"
    ]
  },
  "sprite": {
    "src": "src/img/sprite",
    "dest": "dist/img/sprite"
  },
  "icons": {
    "src": "src/img/icons",
    "dest": "dist/img/icons"
  }
}
